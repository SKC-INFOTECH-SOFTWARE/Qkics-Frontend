{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://qkicsbackend.matchb.online/api/:path*"
    },
    {
      "source": "/media/:path*",
      "destination": "https://qkicsbackend.matchb.online/media/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}